<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="452.0" prefWidth="642.0" styleClass="BG" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyMovieCollection.GUI.controller.MediaPlayerWindowController">
   <children>
      <MediaView fx:id="movieView" fitHeight="350.0" fitWidth="500.0" />
      <HBox layoutY="424.0" prefHeight="29.0" prefWidth="642.0" styleClass="AnchorPane" AnchorPane.bottomAnchor="-1.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="btnPlayPause" mnemonicParsing="false" onAction="#playPauseMovie" stylesheets="@style.css" text="Play">
               <styleClass>
                  <String fx:value="CreateMovieText" />
                  <String fx:value="AnchorPane" />
               </styleClass></Button>
            <Button fx:id="btnStop" mnemonicParsing="false" onAction="#stopMovie" stylesheets="@style.css" text="Stop">
               <styleClass>
                  <String fx:value="AnchorPane" />
                  <String fx:value="CreateMovieText" />
               </styleClass></Button>
            <Button fx:id="btnMute" mnemonicParsing="false" onAction="#muteSound" stylesheets="@style.css" text="Mute">
               <styleClass>
                  <String fx:value="AnchorPane" />
                  <String fx:value="CreateMovieText" />
               </styleClass></Button>
         </children></HBox>
      <Button fx:id="btnClose" layoutX="590.0" layoutY="2.0" mnemonicParsing="false" onAction="#MediaClose" stylesheets="@style.css" text="Close" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <styleClass>
            <String fx:value="CloseRed" />
            <String fx:value="CreateMovieText" />
         </styleClass></Button>
   </children>
</AnchorPane>
